{
    "name": "collab-hive-api",
    "version": "1.0.0",
    "description": "API for Collab Hive platform",
    "main": "index.js",
    "author": "Shreyas Sane",
    "repository": "https://github.com/shreyasdbz/collab-hive-api",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev-basic": "npx nodemon",
        "dev": "npx nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register src/index.ts'",
        "build-old": "rimraf dist && env && npx tsc",
        "build": "npx tsc",
        "start": "npm run build && node dist/index.js",
        "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
        "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,scss,md}'",
        "create-resource": "node src/scripts/create-resource.js"
    },
    "_moduleAliases": {
        "@api": "dist/api",
        "@config": "dist/config",
        "@middlewares": "dist/middlewares",
        "@models": "dist/models",
        "@mock": "dist/mock",
        "@utils": "dist/utils"
    },
    "dependencies": {
        "@prisma/client": "^6.1.0",
        "@supabase/supabase-js": "^2.47.10",
        "@upstash/redis": "^1.34.3",
        "axios": "^1.7.9",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "env-var": "^7.5.0",
        "express": "^4.21.2",
        "express-validator": "^7.2.0",
        "helmet": "^8.0.0",
        "http-errors": "^2.0.0",
        "module-alias": "^2.2.3",
        "pino-http": "^10.3.0",
        "pino-pretty": "^13.0.0",
        "prisma": "^6.1.0",
        "tsconfig-paths": "^4.2.0",
        "uuid": "^11.0.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/http-errors": "^2.0.4",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.2",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.18.2",
        "@typescript-eslint/parser": "^8.18.2",
        "add": "^2.0.6",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.3",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.2",
        "yarn": "^1.22.22"
    }
}
